{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      

        "@*": ["./src/*"],
  "@/*": ["./src/*"],
  "@assets/*": ["./src/assets/*"],
  "@styles/*": ["./src/styles/*"],
  "@pages/*": ["./src/pages/*"],
  "@geii/*": ["./src/pages/geii/*"],
  "@perso/*": ["./src/pages/perso/*"],
  "@travail/*": ["./src/pages/travail/*"],
  "@data/*": ["./src/data/*"],
  "@components/*": ["./src/components/*"],
  "@liste/*": ["./src/components/page/liste/*"],
  "@3d/*": ["./src/components/3d/*"],
  "@page/*": ["./src/components/page/*"],
  "@layout/*": ["./src/components/layout/*"]


        
    },
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,

    /* Bundler mode */
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",

    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src"],
  "exclude": ["node_modules","3d"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
